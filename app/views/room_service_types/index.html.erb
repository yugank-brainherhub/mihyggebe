<p id="notice"><%= notice %></p>

<h1>Room Service Types</h1>
<table class="responsive nowrap table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @room_service_types.each do |room_service_type| %>
      <tr>
        <td><%= room_service_type.name %></td>
        <td><%= link_to 'Show', room_service_type %></td>
        <td><%= link_to 'Edit', edit_room_service_type_path(room_service_type) %></td>
        <td><%= link_to 'Destroy', room_service_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Room Service Type', new_room_service_type_path %>
