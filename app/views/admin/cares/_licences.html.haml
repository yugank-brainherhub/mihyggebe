#licencesModal.modal.fade{"aria-hidden" => "true", "aria-labelledby" => "exampleModalLabel", :role => "dialog", :tabindex => "-1"}
  .modal-dialog{:role => "document"}
    .modal-content
      .modal-header
        %h5#exampleModalLabel.modal-title Care Licences
        %button.close{"aria-label" => "Close", "data-dismiss" => "modal", :type => "button"}
          %span{"aria-hidden" => "true"} Ã—
      .modal-body{style: "max-height:500px;overflow:scroll;"}
        %table.table
          %tr
            %th Licence Type
            %th File 
          - @licences.each do |licence|
            %tr
              %td= licence.licence_type
              %td
                = link_to "Download", rails_blob_path(licence.file, disposition: "attachment")
